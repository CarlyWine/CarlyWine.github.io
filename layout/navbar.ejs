<nav class="navbar navbar-expand-lg navbar-light bg-light fixed-top">
    <a class="navbar-brand" href="#"><%= config.header %></a>
    <button class="navbar-toggler"
            type="button"
            data-toggle="collapse"
            data-target="#navbarNavAltMarkup"
            aria-controls="navbarNavAltMarkup"
            aria-expanded="false"
            aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav">
            <% for (var i=0; i < config.menu.length; i++) { %>
                <% if (config.menu[i].disp == page.layout) { %>
                    <a class="nav-item nav-link active" href="#"><%= config.menu[i].disp %> <span class="sr-only">(current)</span></a>
                <% } else { %>
                    <a class="nav-item nav-link" href="<%= config.menu[i].page %>.html"><%= config.menu[i].disp %></a>
                <% } %>
            <% } %>
        </div>
    </div>
</nav>
